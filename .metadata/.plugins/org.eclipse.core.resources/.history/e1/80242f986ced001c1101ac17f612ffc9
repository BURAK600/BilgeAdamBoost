package bilgeadamweek2;

public class ArraysMaxMin {

	public static void main(String[] args) {
		int x = 10;
		int y = 14;

		System.out.println(x == y ? "Evet" : "Hayir");

		String string = "dflegleflglglglelbbbbbbbbb";
		System.out.println(string.substring(string.length() - 4));
		System.out.println(string.substring(0, string.length() - 4));
		System.out.println(string.substring(5));

	}

}
